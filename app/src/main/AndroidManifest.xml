<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.kaerdan.retainpresenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".single_activity.SingleActivity"
            android:label="@string/single_activity_title"/>

        <activity android:name=".second.SecondActivity"
            android:label="@string/second_activity_title"/>

        <activity android:name=".third.ThirdActivity"
            android:label="@string/third_activity_title"/>

        <activity android:name=".nested_viewpager.FourthActivity"
            android:label="@string/fourth_activity_title"/>
    </application>

</manifest>